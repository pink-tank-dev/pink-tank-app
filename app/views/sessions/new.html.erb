<div class="container">
  <%= form_with url: login_path, method: :post do |form| %>
    <table>
      <tr>
        <td><%= form.label :email %></td>
        <td><%= form.email_field :email %></td>
      </tr>
      <tr>
        <td><%= form.label :password%></td>
        <td><%= form.password_field :password %></td>
      </tr>
    </table>
    <%= form.submit "Login" %>
  <% end %>
  <button type="button" class="btn">
    <%= link_to "Sign Up", new_user_path %>
  </button>
</div>